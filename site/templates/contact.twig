{% extends 'main.twig' %}

{% block content %}

<section class="l-container">
  <form action="" class="form">
    <fieldset>
      <label class="form__label" for="">EMAIL</label>
      <input class="form__input" type="text" placeholder="Enter email address" required />
    </fieldset>
    <fieldset>
    <label class="form__label" for="">Full Name</label>
      <input class="form__input" type="text" placeholder="Enter full name" required />
    </fieldset>
    <fieldset>
      <label class="form__label" for="">PASSWORD</label>
      <input class="form__input" type="text" placeholder="Comments" required />
    </fieldset>
    <input class="form__submit is-btn-hovered" type="submit" value="Send Email">
  </form>
</section>


{% endblock content %}